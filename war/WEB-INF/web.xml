<?xml version="1.0" encoding="utf-8"?>
<web-app xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
xmlns="http://java.sun.com/xml/ns/javaee"
xmlns:web="http://java.sun.com/xml/ns/javaee/web-app_2_5.xsd"
xsi:schemaLocation="http://java.sun.com/xml/ns/javaee
http://java.sun.com/xml/ns/javaee/web-app_2_5.xsd" version="2.5">
    <servlet>
        <servlet-name>parser</servlet-name>
        <servlet-class>edu.drexel.gameailab.ParserServlet</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>nlp</servlet-name>
        <servlet-class>edu.drexel.gameailab.NLPServlet</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>opennlp</servlet-name>
        <servlet-class>edu.drexel.gameailab.OpenNLPServlet</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>berkeley</servlet-name>
        <servlet-class>edu.drexel.gameailab.BerkeleyServlet</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>tagger</servlet-name>
        <servlet-class>edu.drexel.gameailab.TaggerServlet</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>malt</servlet-name>
        <servlet-class>edu.drexel.gameailab.MaltServlet</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>clearnlp</servlet-name>
        <servlet-class>edu.drexel.gameailab.ClearNLPServlet</servlet-class>
    </servlet>



    <servlet-mapping>
        <servlet-name>parser</servlet-name>
        <url-pattern>/parse</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>nlp</servlet-name>
        <url-pattern>/nlp</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>opennlp</servlet-name>
        <url-pattern>/opennlp</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>berkeley</servlet-name>
        <url-pattern>/berkeley</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>tagger</servlet-name>
        <url-pattern>/tagger</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>malt</servlet-name>
        <url-pattern>/malt</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>clearnlp</servlet-name>
        <url-pattern>/clearnlp</url-pattern>
    </servlet-mapping>


    

	<welcome-file-list>
		<welcome-file>index.html</welcome-file>
	</welcome-file-list>
</web-app>
